name:                lesson16
version:             0.1.0.0
author:              Jack Palfrey
maintainer:          jack.palf@gmail.com
cabal-version:       >=1.10
build-type:          Simple


executable lesson16
  hs-source-dirs:      src
  main-is:             Lesson16.hs
